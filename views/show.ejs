<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <a href="/">Add Customer</a>
    <div class="container">
        <table>
            <tr style="padding: 10px;">
                <th style="padding: 15px; ">user id</th>
                <th style="padding: 15px;">user name</th>
                <th style="padding: 15px;">Email</th>
                <th style="padding: 15px;">Password</th>
                <th style="padding: 15px;">Delete</th>
                <th style="padding: 15px;">Update</th>



            </tr>
            <% user.forEach(function(user){%>
                <tr>
                    <td><%=user._id%></td>
                    <td><%=user.name%></td>
                    <td><%=user.email%></td>
                    <td><%=user.password%></td>
                    <td><a href="/delete/<%=user._id%>">Delete</a></td>
                    <td><a href="/edit/<%=user._id%>">Edit</a></td>
                </tr>
                <% })%>
        </table>
    </div>
</body>
</html>